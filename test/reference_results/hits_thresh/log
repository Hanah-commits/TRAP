# split matrix file results.22273/test.balanced.pssm into individual chunks: results.22273/hits_thresh/matrices
/home/manke/tools/TRAP/bin/ANNOTATE_v3.05 -s /home/manke/tools/TRAP/data/test.fa --pssm results.22273/hits_thresh/matrices/matrix_00000 -g 0.5 -t 2.5 | awk 'BEGIN {OFS="\t" } !/^($|#)/ {print $1, $4, $5, ".", int(100*$6), $7}' | sort -k1,1 -k2,2g > results.22273/hits_thresh/bed/test_M00001.bed; /package/UCSCtools/bedToBigBed results.22273/hits_thresh/bed/test_M00001.bed /home/manke/tools/TRAP/data/mm9.fa.fai results.22273/hits_thresh/bigbed/test_M00001.bb
